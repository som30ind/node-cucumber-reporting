<%
  if (typeof message === 'string' && message.trim()) {
    const msgId = counter.next();
%>
  <div class="inner-level">
    <div class="message indention">
      <div data-toggle="collapse" class="<%= isPassed ? 'collapsed ' : '' %>collapsable-control" data-target="#msg-<%= msgId %>">
          <a><%= messageName %></a>
      </div>
      <div id="msg-<%= msgId %>" class="collapse collapsable-details<%= !isPassed ? ' in' : '' %>">
        <pre><%= message %></pre>
      </div>
    </div>
  </div>
<% } %>
